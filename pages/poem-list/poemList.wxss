.listContainer{
    width: 100%;
    height: 100vh;
    background-color: antiquewhite;
    padding-top: 20rpx;
}
.aBox{
    width: 100%;
    /* height: 200rpx; */
    /* border: 1px solid black; */
    margin:10px 0 30px 0;
    display: flex;
}
.avatar{
    width: 10%;
    height: 70rpx;
    background-color: black;
    display: inline-block;
}
.avatar image{
    width: 100%;
    height: 100%;
    background-size: 100% 100%;
}
.content{
    width: 90%;
    height: 100%;
    background-color: blueviolet;
    display: flex;
    padding: 10rpx;
    align-items: center;
    justify-content: center;
    padding: 20rpx;

}
